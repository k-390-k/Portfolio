<%form_with(model: @user) local: true do |f|%>
  <=%f.label :name, '名前'%>
  <=%f.text_field :name%>

  <=%f.label :email, 'メールアドレス'%>
  <=%f.text_field :email%>

  <=%f.label :password, 'パスワード'%>
  <=%f.text_field :password%>

  <=%f.label :password_confirmation, 'もう一度入力してください'%>
  <=%f.text_field :password_confirmation%>

  <=%f.submit '新規登録'%>
<%end%>
